<h1>Users!</h1>

<table>
  <tr>
    <th>ID</th>
    <th>NAME</th>
    <th>TITLE</th>
  </tr>
  <% User.all.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></a></td>
      <td><%= user.title %></td>
    </tr>
  <% end %>
</table>



